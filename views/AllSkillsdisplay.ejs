<div>
    <% if(rs.length > 0) { %>
    <table border="1">
        <tr><th>Skill Name</th></tr>
        <% for (var i=0; rs.length > i; i++) { %>
        <tr>
            <td><a href="/skills/?skill_id=<%= rs[i].skill_id %>"> <%= rs[i].name %></a></td>
        </tr>
        <% } %>
    </table>
    <% } else { %>
    No skills are entered into the Skills database table.
    <% } %>
</div>

<div style="margin-top:10px;">
    <a href="/skills/create">Create New Skill</a>
</div>